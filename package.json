{
  "name": "clipster",
  "displayName": "Clipster",
  "version": "1.1.1",
  "description": "A VS Code extension to copy paths, folder structures, and contents with customization.",
  "icon": "resources/icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onCommand:clipster.createFileFromClipboard",
    "onCommand:clipster.copyFileContentWithHeader",
    "onCommand:clipster.copyFolderStructure",
    "onCommand:clipster.copyFolderStructureAndContent",
    "onCommand:clipster.copyRootFolderPath",
    "onCommand:clipster.copyRootFolderStructure",
    "onCommand:clipster.copyRootFolderStructureAndContent",
    "onCommand:clipster.copyFilesContentWithPaths"
  ],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheJesper/clipster.git"
  },
  "publisher": "conzeon",
  "scripts": {
    "test": "mocha --require @babel/register --require ./test/setup.js --recursive src/**/*.test.js",
    "webpack": "webpack --mode development",
    "build": "webpack --config webpack.config.cjs --mode production --devtool hidden-source-map",
    "clean": "rimraf ./out",
    "bump-build": "standard-version --no-verify && npm run build",
    "clean-build-install": "npm run clean && npm run bump-build && npm run install-extension",
    "install-extension": "node build.js",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-transform-modules-commonjs": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/register": "^7.25.7",
    "@types/vscode": "^1.93.0",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.15.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.56.0",
    "esm": "^3.2.25",
    "mocha": "^10.7.3",
    "mock-fs": "^5.3.0",
    "mock-require": "^3.0.3",
    "rimraf": "^3.0.2",
    "sinon": "^19.0.2",
    "standard-version": "^9.3.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "vscode": "^1.93.0"
  },
  "dependencies": {
    "ignore": "^6.0.2",
    "picomatch": "^4.0.2"
  },
  "contributes": "added by buildscript"
}
